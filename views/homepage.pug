doctype html
html
  head
    title= title
    link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css") 
  body
    .container
      h1 Custom Object Table
      a.btn.btn-primary(href="/update-cobj") Add/Update this table
      table.table.table-striped.table-bordered.table-hover
        thead
          tr
            th Name
            th Book
            th Edition
        tbody
          each contact in data
            tr
              td= contact.properties.author_name || 'N/A'
              td= contact.properties.book || 'N/A'
              td= contact.properties.edition || 'N/A'
    
    script(src="https://code.jquery.com/jquery-3.5.1.slim.min.js")
    script(src="https://cdn.jsdelivr.net/npm/@popperjs/core@1.16.0/dist/umd/popper.min.js")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js") 
